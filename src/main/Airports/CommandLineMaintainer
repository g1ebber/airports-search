package Airports;

import java.util.Scanner;

public final class CommandLineMaintainer {

    public final String getSubstring() {
        System.out.println("Введите строку:");
        Scanner console = new Scanner(System.in);

        return console.nextLine();
    }

    public final void usedTime(double time) {
        System.out.println("Время затраченное на поиск: " + time );
    }

    public final void usedTime(int countMatches) {
        System.out.println("Количество найденных строк: " + countMatches );
    }
}